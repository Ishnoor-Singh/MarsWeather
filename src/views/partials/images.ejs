<div class="big-sol">
    <div class="images-container">
        <% for (var i = 0; i < images.length; i++) { %>
        <div class="col-md-4">
            <div class="thumbnail">
                <img src="<%= images[i].img_src %>">
                <div class="caption">
                    <h3><%= images[i].rover.name %></h3>
                    <p><%= images[i].camera.full_name %></p>
                </div>
            </div>
        </div>
        <% } %>
    </div>

    <% if (pages > 0) { %>
    <ul class="pagination text-center">
        <% if (current == 1) { %>
        <li class="disabled"><a>First</a></li>
        <% } else { %>
        <li><a href="/sol/<%= sol %>/page/1">First</a></li>
        <% } %>
        <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
        <% if (i !== 1) { %>
        <li class="disabled"><a>...</a></li>
        <% } %>
        <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
        <% if (i == current) { %>
        <li class="active"><a><%= i %></a></li>
        <% } else { %>
        <<<<<<< HEAD <li><a href="/<%= sol %>/<%= i %>"><%= i %></a></li>
            =======
            <li><a href="/sol/<%= sol %>/page/<%= i %>"><%= i %></a></li>
            >>>>>>> done with UI
            <% } %>
            <% if (i == Number(current) + 4 && i < pages) { %>
            <li class="disabled"><a>...</a></li>
            <% } %>
            <% } %>
            <% if (current == pages) { %>
            <li class="disabled"><a>Last</a></li>
            <% } else { %>
            <<<<<<< HEAD <li><a href="/<%= sol %>/<%= pages %>">Last</a></li>
                =======
                <li><a href="/sol/<%= sol %>/page/<%= pages %>">Last</a></li>
                >>>>>>> done with UI
                <% } %>
    </ul>
    <% } %>
</div>