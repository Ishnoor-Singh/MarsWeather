<div class="big-sol">
    <div class="images-container">
        <% for (var i = 0; i < images.length; i++) { %>
        <div class="col-md-4">
            <div class="thumbnail" style="height: 720px;display: flex; flex-direction: column;">
                <a href="<%= images[i].img_src %>" style="flex-grow: 1; display: flex; ">
                    <img src="<%= images[i].img_src %>" style="flex-grow: 1;">
                </a>
                <div class=" caption" style="">
                    <h3><%= images[i].rover.name %></h3>
                    <p><%= images[i].camera.full_name %></p>
                </div>
            </div>
        </div>
        <% } %>
    </div>
    <% if (pages > 0) { %>
    <ul class="pagination text-center">
        <% if (current == 1) { %>
        <li class="disabled"><a>First</a></li>
        <% } else { %>
        <li><a href="/sol/<%= sol %>/page/1/total/<%= pages %>">First</a></li>
        <% } %>
        <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
        <% if (i !== 1) { %>
        <li class="disabled"><a>...</a></li>
        <% } %>
        <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
        <% if (i == current) { %>
        <li class="active"><a><%= i %></a></li>
        <% } else { %>
        <li><a href="/sol/<%= sol %>/page/<%= i %>/total/<%= pages %>"><%= i %></a></li>
        <% } %>
        <% if (i == Number(current) + 4 && i < pages) { %>
        <li class="disabled"><a>...</a></li>
        <% } %>
        <% } %>
        <% if (current == pages) { %>
        <li class="disabled"><a>Last</a></li>
        <% } else { %>
        <li><a href="/sol/<%= sol %>/page/<%= pages %>/total/<%= pages %>">Last</a></li>
        <% } %>
    </ul>
    <% } %>
</div>